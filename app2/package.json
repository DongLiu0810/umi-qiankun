{
  "version": "1.0.0",
  "name": "app2",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  "scripts": {
    "start": "cross-env SERVERHTTP=xxxxxxxx umi dev",
    "start:release": "cross-env SERVERHTTP=xxxxxxx umi dev",
    "start:production": "cross-env SERVERHTTP=xxxxxxx umi dev",
    "build": "umi build",
    "analysis": "ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepare": "npx husky install",
    "lint": "lint-staged",
    "check-eslint": "eslint --fix --ext .tsx src",
    "check-stylelint": "stylelint '**/*.less' --fix --custom-syntax postcss-less",
    "release": "standard-version",
    "commit": "git add . && npm run lint && npm run check-eslint && npm run check-stylelint && git cz",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "ahooks": "^2.10.0",
    "antd": "^4.21.6",
    "compression-webpack-plugin": "^5.0.1",
    "cross-env": "^7.0.3",
    "echarts-for-react": "^3.0.2",
    "js-cookie": "^3.0.1",
    "loader-utils": "^3.2.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.21",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.179",
    "@types/md5": "^2.3.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@umijs/plugin-qiankun": "^2.37.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog-zh": "0.0.2",
    "eslint": "^8.15.0",
    "eslint-config-egg": "^11.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.1.2"
  }
}
